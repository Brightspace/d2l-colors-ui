{
  "name": "vui-colors",
  "version": "0.1.0",
  "description": "Variables for the VUI color palette.",
  "scripts": {
    "build:autoprefix": "postcss --use autoprefixer ./sample/colors.css -o ./sample/colors.css",
    "build:css": "node-sass --output-style expanded ./sample/colors.css.scss ./sample/colors.css && npm run build:autoprefix",
    "test": "npm run build:css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-colors.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Brightspace/valence-ui-colors/issues"
  },
  "homepage": "https://github.com/Brightspace/valence-ui-colors",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "node-sass": "^3.1.1",
    "postcss-cli": "^1.3.1"
  }
}
